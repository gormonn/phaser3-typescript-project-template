{
    "name": "phaser3-typescript-project-template",
    "version": "1.0.2",
    "description": "A Phaser 3 Project Template using Rollup and TypeScript",
    "main": "src/game.ts",
    "scripts": {
        "dev": "rollup --config rollup.config.dev.js",
        "build": "rollup --config rollup.config.dist.js",
